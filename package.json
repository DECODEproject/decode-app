    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./testenv.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
